;   param=value ; assign values to parameters

config_version=5

[application]

config/name="UI Project"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
GameData="*res://global/singleton/game_data.gd"
GlobalSound="*res://global/singleton/global_sound.gd"
MapState="*res://scenes/map_components/script/map_state.gd"

[dialogic]

directories/dch_directory={
"weird_cat": "res://dialogic_timeline/character/weird_cat.dch"
}
directories/dtl_directory={
"finding_first_job": "res://dialogic_timeline/finding_first_job.dtl",
"finding_first_job_2": "res://dialogic_timeline/finding_first_job_2.dtl",
"got_out_side_1st_time": "res://dialogic_timeline/got_out_side_1st_time.dtl"
}
variables={
"weird_cat_question_1": ""
}
glossary/default_case_sensitive=true
layout/style_list=["res://assets/ui_theme/dialogic_style.tres"]
